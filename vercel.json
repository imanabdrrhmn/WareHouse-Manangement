{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/routes/Auth.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "/routes/users.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "/routes/products.js"
    },
    {
      "src": "/files/(.*)",
      "dest": "/routes/files.js"
    },
    {
      "src": "/orders/(.*)",
      "dest": "/routes/orders.js"
    },
    {
      "src": "/inventory/(.*)",
      "dest": "/routes/inventory.js"
    },
    {
"rewrites": [
        {
      "src": "/(.*)",
      "dest": "/app.js"
        }
    ]
    }
  ]
}
